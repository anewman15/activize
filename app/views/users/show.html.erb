<% home_page_links(current_user).each do |link| %>
<article class="media">
  <figure class="media-left">
    <div class="image is-64x64">
      <img src="https://bulma.io/images/placeholders/128x128.png">
    </div>
  </figure>
  <div class="media-content">
    <div class="content">
      <h2 class="title is-3"><%= link %></h2>
    </div>
  </div>
</article>
<% end %>
